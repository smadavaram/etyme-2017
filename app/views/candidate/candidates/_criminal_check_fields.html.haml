.multi-field-container
  .row
    .col-sm-4
      .form-group
        %label State
        = f.text_field :state, placeholder: "State",class: 'form-control input'

    .col-sm-3
      .form-group
        %label Address
        = f.text_field :address,placeholder: "Address",class: 'form-control education_start_year p-location input'

    .col-sm-2
      .form-group
        %label Start Date
        = f.text_field :start_date,placeholder: "YYYY-MM-DD",class: 'form-control p_date creminal_check_start_year input'

    .col-sm-2
      .form-group
        %label End Date
        = f.text_field :end_date,placeholder: "YYYY-MM-DD",class: 'form-control p_date creminal_check_end_year input'
    .col-md-1
      .form-group
        %label Delete
        %a.bttn.btn-add.remove-multi-fields.pull-left.delete_icon
          %i.fa.fa-trash

        

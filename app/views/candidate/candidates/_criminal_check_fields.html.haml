.row.multi-field-container{style: "padding: 0px 15px 15px 15px;"}
  .row
    .col-sm-3
      .form-group
        %p State
        = f.text_field :state, placeholder: "State",class: 'form-control p-location input'

    .col-sm-3
      .form-group
        %p Address
        = f.text_field :address,placeholder: "Address",class: 'form-control education_start_year p-location input'

    .col-sm-3
      .form-group
        %p Start Date
        = f.text_field :start_date,placeholder: "MM/DD/YY",class: 'form-control education_start_year p-location input'

    .col-sm-3
      .form-group
        %p End Date
        = f.text_field :end_date,placeholder: "MM/DD/YY",class: 'form-control education_start_year p-location input'

  .row
    .col-md-12
      %a.bttn.btn-add.remove-multi-fields.pull-right.delete_icon
        %i.fa.fa-trash

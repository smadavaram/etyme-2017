.row.multi-field-container{style: "padding: 0px 15px 15px 15px;"}
  .row
    .col-sm-3
      .form-group
        %p Certificate Title
        = f.text_field :title,placeholder: "Certificate Title",class: 'form-control p-location input'
  
    .col-sm-3
      .form-group
        %p Institute
        = f.select :institute, industry_list,{},{class: 'form-control p-location input',style: "width:100% !important"}

    .col-sm-3
      .form-group 
        %p Start Date
        = f.text_field :start_date,placeholder: "MM-DD-YY",class: 'form-control start_date p-location input'
  
    .col-sm-3
      .form-group
        %p End Date
        = f.text_field :end_date,placeholder:"MM-DD-YY",class: 'form-control end_date p-location input'

  .row
    .col-md-12
      %a.bttn.btn-add.remove-multi-fields.pull-right.delete_icon
        %i.fa.fa-trash


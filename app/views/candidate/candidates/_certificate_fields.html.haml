.row.multi-field-container{style: "padding: 0px 15px 15px 15px;"}
  .row
    .col-sm-6
      .form-group
        = f.text_field :title,placeholder: "Certificate Title",class: 'form-control p-location input-lg'
  
    .col-sm-6
      .form-group
        = f.select :institute, industry_list,{},{class: 'form-control p-location input-lg',style: "width:100% !important"}

  .row
    .col-sm-3
      .form-group
        = f.text_field :start_date,placeholder: "MM-DD-YY",class: 'form-control start_date p-location input-lg'
  
    .col-sm-3
      .form-group
        = f.text_field :end_date,placeholder:"MM-DD-YY",class: 'form-control end_date p-location input-lg'

  .row
    .col-md-12
      %a.bttn.btn-add.remove-multi-fields.pull-right.delete_icon
        %i.fa.fa-trash

:javascript
  $('.start_date, .end_date').datepicker({
   autoclose: true,
   todayHighlight: true,
   format: "mm-dd-yy"
  });         
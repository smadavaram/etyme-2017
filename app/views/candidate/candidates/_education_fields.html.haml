.row.multi-field-container{style: "padding: 0px 15px 15px 15px;"}
  .row
    .col-sm-3
      .form-group
        %p Degree level
        = f.text_field :degree_level, placeholder: "Degree level", class: 'form-control input'
  
    .col-sm-3
      .form-group
        %p Institute
        = f.select :institute, industry_list,{},{class: 'form-control input',style: "width:100% !important"}
    .col-sm-3
      .form-group
        %p Degree Title
        = f.text_field :degree_title, placeholder: "Degree Title",class: 'form-control input'
    .col-sm-3
      .form-group
        %p CGPA
        = f.text_field :degree_title, placeholder: "CGPA", class: 'form-control input'
  .row 
    .col-md-3
      .form-group
        %p Start Year
        = f.text_field :start_year, placeholder: "YYYY-MM-DD", class: 'form-control ed_start_year'

    .col-md-3
      .form-group
        %p End Year
        = f.text_field :completion_year, placeholder:"YYYY-MM-DD", class: 'form-control ed_end_year input'

  .row
    .col-md-12
      .form-group
        = f.fields_for :candidate_education_document, f.object.candidate_education_document do |b|
          = render 'candidate/candidates/candidate_education_document_fields', f: b
        .links.education_links.pull-right
          = link_to_add_association '+ Add Document', f, :candidate_education_document, class: 'btn btn-success add_education_btn', partial: 'candidate/candidates/candidate_education_document_fields'


- content_for :scripts do
  :javascript 

    $(document).ready(function() {
      $('.ed_start_year, .ed_end_year').datepicker({
       dateFormat: "yy-mm-dd"
      });  
    });
          
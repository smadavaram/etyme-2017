- if @tab.present? && !@tab.eql?("fln_basic_info")
  $("##{@tab.gsub('_', '-')}-form").html("#{j render 'candidate/candidates/partials/'+@tab.gsub('-', '_')+'_form'}")
  $(".select2").select2({placeholder: "Select a User(s)"});
  $(".file_select").select2({placeholder: "Select Company Document"});
  $("##{@tab.gsub('_', '-')}-form .can-filepicker-attach").each(function(e){
  $(this).unbind();
  });
  $('.p_date').datepicker({
  dateFormat: "mm-dd-yy"
  });
  addressLookup();
  $("input[name='candidate[designation_status]']").click(function(){
  if($("input[name='candidate[designation_status]']:checked").val() == "Employee"){
  $('#employee-designations').removeClass('display-none');
  }
  else if($("input[name='candidate[designation_status]']:checked").val() == "Freelancer"){
  $('#employee-designations').addClass('display-none');
  }
  });

-if flash[:errors].present?
  - flash[:errors].each do |e|
    flash_error("#{e}");
- if flash[:success].present?
  flash_success("#{flash[:success]}");
.padded-lg
  = render 'shared/plugins_index'
  .element-box
    .margin-top-10.main_accordion
      %a.sidebar_collapsible.side-bar_tab.active
        %div.header-accordion CMS
      .w-100.text-justify.nav-sidebar-collaps{style: 'display:block'}
        %div.bg-color-white
          .row.align-item-center
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedModal", "data-toggle" => "modal", href: '#embedModal'}
                  .icon-part
                    .fa.fa-briefcase
                  .link-part Job Feed
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedRssModal", "data-toggle" => "modal", href: '#embedModal'}
                  .icon-part
                    .fa.fa-briefcase
                  .link-part Job Rss Feed

            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedJobModal", "data-toggle" => "modal", href: '#embedJobModal'}
                  .icon-part
                    .fa.fa-briefcase
                  .link-part Company Job Feed

            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedJobRssModal", "data-toggle" => "modal", href: '#embedJobModal'}
                  .icon-part
                    .fa.fa-briefcase
                  .link-part Company Job Rss Feed

            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedProductModal", "data-toggle" => "modal", href: '#embedProductModal'}
                  .icon-part
                    .fa.fa-product-hunt
                  .link-part Product Feed
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedProductRssModal", "data-toggle" => "modal", href: '#embedProductModal'}
                  .icon-part
                    .fa.fa-product-hunt
                  .link-part Product Rss Feed

            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedServiceModal", "data-toggle" => "modal", href: '#embedServiceModal'}
                  .icon-part
                    .fa.fa-street-view
                  .link-part Service Feed
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedServiceRssModal", "data-toggle" => "modal", href: '#embedServiceModal'}
                  .icon-part
                    .fa.fa-street-view
                  .link-part Service Rss Feed

            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedBlogModal", "data-toggle" => "modal", href: '#embedBlogModal'}
                  .icon-part
                    .fa.fa-rss
                  .link-part Blog Feed
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedBlogRssModal", "data-toggle" => "modal", href: '#embedBlogModal'}
                  .icon-part
                    .fa.fa-rss
                  .link-part Blog Rss Feed
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedBenchModal", "data-toggle" => "modal", href: '#embedBlogModal'}
                  .icon-part
                    .fa.fa-user
                  .link-part Bench Feed
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedBenchRssModal", "data-toggle" => "modal", href: '#embedBlogModal'}
                  .icon-part
                    .fa.fa-user
                  .link-part Bench Rss Feed


    .margin-top-10.main_accordion
      %a.sidebar_collapsible.side-bar_tab.active
        %div.header-accordion Collaboration
      .w-100.text-justify.nav-sidebar-collaps{style: 'display:block'}
        %div.bg-color-white
          .row.align-item-center
            -Plugin.plugin_types.keys.each do |plugin_type|
              -unless current_company.plugins.send(plugin_type).present?
                - if plugin_type == "skype"
                  .col-md-3.col-6
                    .feed
                      %a.modal-opener.display-flex{"data-target" => "#skype_integration_modal", "data-toggle" => "modal", href: '#skype_integration_modal'}
                        .icon-part
                          .fa.fa-skype
                        .link-part Integrate #{plugin_type}
                - if plugin_type == "zoom"
                  .col-md-3.col-6
                    .feed
                      %a.modal-opener.display-flex{:href => plugin_company_company_path(current_company,plugin_type: plugin_type)}
                        .icon-part
                          .fa.fa-video-camera
                        .link-part Integrate #{plugin_type}

    .margin-top-10.main_accordion
      %a.sidebar_collapsible.side-bar_tab.active
        %div.header-accordion Workflows
      .w-100.text-justify.nav-sidebar-collaps{style: 'display:block'}
        %div.bg-color-white
          .row.align-item-center
            -Plugin.plugin_types.keys.each do |plugin_type|
              -unless current_company.plugins.send(plugin_type).present?
                -if plugin_type == "docusign"
                  .col-md-4.col-6
                    .feed
                      %a.modal-opener.display-flex{:href => plugin_company_company_path(current_company,plugin_type: plugin_type)}
                        .icon-part
                          = image_tag('/docusign.png', width: '35', height: '35')
                        .link-part Integrate #{plugin_type}

            .col-md-4.col-6
              .feed
                %a.modal-opener.display-flex{"data-target" => "#embedProductModal", "data-toggle" => "modal", href: '#embedProductModal'}
                  .icon-part
                    = image_tag('qb.png', width: '30', height: '30')
                  .link-part QickBooks


    .margin-top-10.main_accordion
      %a.sidebar_collapsible.side-bar_tab.active
        %div.header-accordion Alerts
      .w-100.text-justify.nav-sidebar-collaps{style: 'display:block'}
        %div.bg-color-white
          .row.align-item-center
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{href: '#'}
                  .icon-part
                    .fa.fa-code-fork
                  .link-part Firebase

    .margin-top-10.main_accordion
      %a.sidebar_collapsible.side-bar_tab.active
        %div.header-accordion Reporting
      .w-100.text-justify.nav-sidebar-collaps{style: 'display:block'}
        %div.bg-color-white
          .row.align-item-center
            .col-md-3.col-6
              .feed
                %a.modal-opener.display-flex{href: '#'}
                  .icon-part
                    .fa.fa-signal
                  .link-part PowerBI



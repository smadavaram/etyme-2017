.content-i
  .content-box
    .row
      .col-sm-12
        .element-wrapper
          .element-box-tp
            .controls-above-table
              .row.mr-1.ml-1
                .col-sm-6
                  %a.btn.btn-sm.btn-secondary{ :href => "/contacts", :title => "Back" } Go Back
                .col-sm-6
            .table-wrapper
              .table-responsive
                %table.table.table-bordered.text-center.table-lg.table-v2.table-striped
                  %thead
                    %tr
                      %th Profile Picture
                      %th Name
                      %th Email
                      %th Gender
                  - @contacts.each do |contact|
                    %tr
                      %td
                      %td
                        = contact[:name]
                      %td
                        = contact[:email]
                      %td
                        = contact[:gender]

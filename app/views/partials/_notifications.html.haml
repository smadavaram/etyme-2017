%ul.notification-body.m-2
  - notifications.each do |n|
    %li
      %span
        %a
          -#= image_tag'default_user.png', alt: '', class: 'air air-top-left margin-top-5', width: '20', height: '20'
          %span.from.icon-paperclip
            = n.title.try(:titleize)
          %time
            = time_ago_in_words(n.created_at)+' ago'
          %span.msg-body
            = raw(n.message)
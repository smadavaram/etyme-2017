.timesheet
  .row
    .col-md-12
      %h1 Pay Bills
      %form.form-horizontal
        .timesheet-header.clearfix
          .pull-left
            .plain-select
              %select
                %option 1
                %option 1
                %option 1
                %option 1
            %input{:placeholder => "Select Date", :type => "text"}/
          .pull-right
            %span Total hour
            %p 0:00
        .table-responsive
          %table#mytable.table.table-bordred
            %thead
              %th #
              %th{:width => "40%"} Details
              %th SAT 10
              %th SAT 11
              %th SAT 12
              %th SAT 13
              %th SAT 14
              %th SAT 15
              %th SAT 16
              %th Total
              %th Action
            %tbody
              %tr
                %td 1
                %td
                  .plain-select
                    %select
                      %option Customername
                      %option 1
                      %option 1
                      %option 1
                  .plain-select
                    %select
                      %option service item
                      %option 1
                      %option 1
                      %option 1
                  %br/
                  %textarea
                  %label.fancy
                    %input.set-checkbox{:type => "checkbox"}/
                    Bilable
                %td
                  %input.checkthis{:type => "text"}/
                %td
                  %input.checkthis{:type => "text"}/
                %td
                  %input.checkthis{:type => "text"}/
                %td
                  %input.checkthis{:type => "text"}/
                %td
                  %input.checkthis{:type => "text"}/
                %td
                  %input.checkthis{:type => "text"}/
                %td
                  %input.checkthis{:type => "text"}/
                %td
                %td
                  %p{"data-placement" => "top", "data-toggle" => "tooltip", :title => "Delete"}
                    %button.btn.btn-danger.btn-xs{"data-target" => "#delete", "data-title" => "Delete", "data-toggle" => "modal"}
                      %span.glyphicon.glyphicon-trash
            %tfoot
              %tr
                %td
                %td
                %td
                %td
                %td
                %td
                %td
                %td
                %td Total
                %td 00:00
                %td
      .timesheet-btn
        %a.btn.btn-success Add
        %a.btn.btn-danger clear
    color: #3E4B5B;
    cursor: pointer;
    border: none;
    text-align: left;
    outline: none;
    font-size: 15px;
    transition: 0.4s;
  }

  .nav-sidebar-collaps {
    display: none;
    box-shadow: none;
  }

  .chat-collaps {
    display: none;
  }

  .sidebar_collapsible:after {
    content: '\002B';
    color: rgba(0, 0, 0, 0.5);
    font-weight: bold;
    float: right;
    margin-left: 5px;
  }

  .active:after {
    color: rgba(0, 0, 0, 0.5);
    content: "\2212";

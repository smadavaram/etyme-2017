<% if flash[:errors].present? %>
<% flash[:errors].each do |e| %>
flash_error("<%= e %>");
<% end %>
<% end %>
<% if flash[:success].present? %>
flash_success("<%= flash[:success] %>");
<% end %>
<% unless @buy_send_document.errors.present? %>
$('#buy_send_documents_table').html("<%= j render 'company/contracts/partials/buy_send_document' , buy_send_document: @buy_send_document%>");
$('#buy_send_documents_modal').modal('hide')
<% end %>
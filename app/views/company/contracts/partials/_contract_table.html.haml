.table-responsive
  %table#resultTable.table.table-striped.table-bordered.table-hover
    %thead
      %tr
        %th{:style => "width:30px"} Pic
        %th Recipient Name
        %th Company
        %th Job
        %th Email / Username
        %th Created By
        %th Phone
        %th Expiry
        %th Status
    %tbody
      - data.each do |contract|
        %tr{class: "#{set_background_status_color(contract.status)}"}
          %td= image_tag contract.try(:job_application).try(:job_invitation).try(:recipient).try(:recipient).try(:photo), alt: '', width: '20'
          %td=# link_to contract.try(:job_application).try(:job_invitation).try(:recipient).try(:recipient).try(:full_name), '#'
          %td= #link_to contract.try(:job_application).try(:job_invitation).try(:recipient).try(:recipient).try(:company.name) , '#'
          %td=# link_to contract.job.title , '#'
          %td
            = #invitation.recipient.email
            %a.pull-right{:href => "javascript:void(0);"}
              %i.fa.fa-key
          %td= link_to contract.created_by.full_name , '#'
          %td= #contract.job_application.job_invitation.recipient.recipient.phone
          %td=# invitation.expiry
          %td
            %span.label.label-success= contract.status

.collaborators
  .mini_chat_users.text-left
    - approvals.sample(4).each do |approval|
      .avatar
        =user_image(approval.user, style: 'width: 35px; height: 35px;', class: 'data-table-image mr-2',title: "#{approval.user.full_name}" )

    - if approvals.count > 4
      .more
        = "+ #{(approvals.count-4).abs} more"

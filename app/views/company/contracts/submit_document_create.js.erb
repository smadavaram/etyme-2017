<% if flash[:errors].present? %>
<% flash[:errors].each do |e| %>
flash_error("<%= e %>");
<% end %>
<% end %>
<% if flash[:success].present? %>
flash_success("<%= flash[:success] %>");
<% end %>
<% unless @sell_document.errors.present? %>
$('#submit_doc').html("<%= j render 'company/contracts/submit_document_table' , sell_request: @sell_document%>");
$('#submit_document').modal('hide')
<% end %>
$(".exp_date").datepicker({
  dateFormat : 'dd/mm/yy'
});
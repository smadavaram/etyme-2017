- current_company.sent_contracts.each do |c|
  .highchart-graph{:style => "min-width: 310px; height: 250px; margin: 0 auto", id: "#{c.id}"}
  -if c.timesheet_logs.present?
    = render 'company/users/graph',contract: c, timesheet_logs_dates: c.timesheet_logs.collect{|tl| tl.transaction_day.strftime("%D")},timesheet_logs_total_time_array:c.timesheet_logs_total_time_array







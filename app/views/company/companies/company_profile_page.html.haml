.with-side-panel.cmp-profile
  .content-i
    .content-box
      .row
        .col-sm-5
          .user-profile.compact
            .up-head-w{:style => "background-image:url(#{current_company.logo })"}
              .up-social
                %a{:href => "#"}
                  %i.os-icon.os-icon-twitter
                %a{:href => "#"}
                  %i.os-icon.os-icon-facebook
              .up-main-info
                %h2.up-header
                  =current_company.full_name
                %h6.up-sub-header
                  Product Designer at Facebook
              %svg.decor{:height => "219px", :preserveaspectratio => "xMaxYMax meet", :version => "1.1", :viewbox => "0 0 842 219", :width => "842px", :xmlns => "http://www.w3.org/2000/svg", "xmlns:xlink" => "http://www.w3.org/1999/xlink"}
                %g{:fill => "#FFFFFF", :transform => "translate(-381.000000, -362.000000)"}
                  %path.decor-path{:d => "M1223,362 L1223,581 L381,581 C868.912802,575.666667 1149.57947,502.666667 1223,362 Z"}

            .up-contents
              .m-b
                .row.m-b
                  .col-sm-6.b-r.b-b
                    = link_to jobs_path do
                      .el-tablo.centered.padded-v
                        .value
                          = current_company.jobs.not_system_generated.count
                        .label
                          Job(s)
                  .col-sm-6.b-b
                    = link_to company_contacts_company_companies_path do
                      .el-tablo.centered.padded-v
                        .value
                          = current_company.company_contacts.count
                        .label
                          contact(s)
                .padded
                  .os-progress-bar.primary
                    .bar-labels
                      .bar-label-left
                        %span Email
                      .bar-label-right
                        %span.text-success #{ current_user.confirmed_at.present? ? "verified" : "Unverified"}
                  .os-progress-bar.primary
                    .bar-labels
                      .bar-label-left
                        %span Phone
                      .bar-label-right
                        %span.text-success #{ current_company.is_number_verify ? "verified" : "Unverified"}
                  .os-progress-bar.primary
                    .bar-labels
                      .bar-label-left
                        %span Owner
                      .bar-label-right
                        %span.text-success #{ current_company.owner_verified ? "verified" : "Unverified"}
          .element-wrapper
            .element-box
              -if @activities.present?
                .d-flex.justify-content-between.align-items-center.mb-3.element-header
                  %h6 User Activity
                  =link_to "View All", jobs_path, class: "link-muted font-xs"
                .timed-activities.compact
                  .timed-activity
                    .ta-date
                      %span Recent Activities
                    .ta-record-w
                      - @activities.each do |activity|
                        = render_activity activity, index: params[:index]
              -else
                %h6.element-header User Activity
                .timed-activities.compact
                  There is no activities.

        .col-sm-7.job-table
          .element-wrapper
            -if @jobs.present?
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Job(s)
                =link_to "View All", jobs_path, class: "link-muted font-xs"
              .element-box
                .table-responsive
                  %table.table.table-lightborder
                    %thead
                      %tr
                        %th
                          Title
                        %th
                          Location
                        %th.text-center
                          Posted By
                    %tbody
                      - @jobs.each do |j|
                        %tr
                          %td.nowrap=link_to do_ellipsis(j.title), job_path(j)
                          %td=j.location
                          %td.text-right=j.created_by.try(:full_name)
            - else
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Job(s)
                =link_to "Create New", jobs_path, class: "link-muted font-xs"
              .element-box.p-3.text-center
                %h6.mb-0 There is no Job Right now.
          .element-wrapper
            -if @benches.present?
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Bench
                =link_to "View All", company_bench_jobs_path, class: "link-muted font-xs"
              .element-box
                .table-responsive
                  %table.table.table-lightborder
                    %thead
                      %tr
                        %th Title
                        %th Name
                        %th.text-center Location
                    %tbody
                      -@benches.each do |b|
                        %tr
                          %td.nowrap= b.candidate.candidate_title
                          %td= b.candidate.try(:full_name)
                          %td.text-right= b.candidate.try(:location)
            - else
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Bench
                =link_to "Create New", jobs_path, class: "link-muted font-xs"
              .element-box.p-3.text-center
                %h6.mb-0 There is no Bench Right now.
          .element-wrapper
            -if @training.present?
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Training(s)
                =link_to "View All", jobs_path, class: "link-muted font-xs"
              .element-box
                .table-responsive
                  %table.table.table-lightborder
                    %thead
                      %tr
                        %th Title
                        %th Location
                        %th.text-center Posted By
                    %tbody
                      - @training.each do |t|
                        %tr
                          %td.nowrap= link_to do_ellipsis(t.title), job_path(t)
                          %td= t.location
                          %td.text-right= t.created_by.full_name
            - else
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Training(s)
                =link_to "Create New", jobs_path, class: "link-muted font-xs"
              .element-box.p-3.text-center
                %h6.mb-0 There is no Training Right now.
          .element-wrapper
            -if @products.present?
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Product(s)
                =link_to "View All", jobs_path, class: "link-muted font-xs"
              .element-box
                .table-responsive
                  %table.table.table-lightborder
                    %thead
                      %tr
                        %th Title
                        %th Location
                        %th.text-center Posted By
                    %tbody
                      - @products.each do |p|
                        %tr
                          %td.nowrap=do_ellipsis(p.title)
                          %td=p.location
                          %td.text-right=p.created_by.full_name
            - else
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Product(s)
                =link_to "Create New", jobs_path, class: "link-muted font-xs"
              .element-box.p-3.text-center
                %h6.mb-0 There is no Products Right now.
          .element-wrapper
            - if @services.present?
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Service(s)
                =link_to "View All", jobs_path, class: "link-muted font-xs"
              .element-box
                .table-responsive
                  %table.table.table-lightborder
                    %thead
                      %tr
                        %th Title
                        %th Location
                        %th.text-center Posted By
                    %tbody
                      - @services.each do |s|
                        %tr
                          %td.nowrap= do_ellipsis(s.title)
                          %td= s.location
                          %td.text-right= s.created_by.full_name
            - else
              .d-flex.justify-content-between.align-items-center.mb-3.element-header
                %h6.mb-0 Service(s)
                =link_to "Create New", jobs_path, class: "link-muted font-xs"
              .element-box.p-3.text-center
                %h6.mb-0 There is no Services Right now.

    .content-panel
      .content-panel-close
        %i.os-icon.os-icon-close
      .element-wrapper
        - if @directories.present?
          .d-flex.justify-content-between.align-items-center.mb-3.element-header
            %h6.mb-0 Office Directory
            = link_to "View All", directories_path, class: "link-muted font-xs"
          .element-box-tp
            - @directories.each do |d|
              .profile-tile
                %a.profile-tile-box{:href => company_user_profile_path(d)}
                  .pt-avatar-w
                    .pt-user-name= user_image(d, style: 'width: 35px; height: 35px;', class: 'data-table-image mr-1',title: "#{d.full_name}" )
                  .pt-user-name= d.full_name
                .profile-tile-meta= contact_widget(d.email, d.phone, d, chat_link: chat_link(d))
        - else
          .d-flex.justify-content-between.align-items-center.mb-3.element-header
            %h6.mb-0 Office Directory
            .element-box-tp
              There is no directory.

      .element-wrapper
        - if @clients.present?
          .d-flex.justify-content-between.align-items-center.mb-3.element-header
            %h6 Clients
            = link_to "View All", network_path, class: "link-muted font-xs"
          .element-box-tp
            .users-list-w
              - @clients.each do |c|
                - company = c.prefer_vendor
                .user-w.with-status.status-green
                  .user-avatar-w
                    .user-avatar= image_tag show_recipient_picture(company.try(:logo))
                  .user-name
                    %h6.user-title= company.full_name rescue ""
                    .data-table-icons= contact_widget(company.owner.email, company.owner.phone, company.owner, chat_link: chat_link(company.owner))

        - else
          %h6.mb-0 Clients
          .element-box-tp
            There is no client.

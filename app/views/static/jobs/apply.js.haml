
-if params[:status] == 'true'
  $('.modals').empty().append(("#{ j render('partials/job_application_modal' , job: @job , job_application: @job_application )}"));
  $('#job-apply-#{@job.id }').modal('toggle');
-else
  $('.modals').empty().append(("#{ j render('static/candidates/partials/form')}"));
  $('#new-candidate').modal('toggle')

var element = $('#accept-filepiker');
filepicker.constructWidget(element);

$( ".file-pick" ).on('click',function() {

$('.fp__overlay').css({'z-index':'99999'});
});
var reqelement = $('#req-accept-filepiker');
filepicker.constructWidget(reqelement);
$( ".req-file-pick" ).on('click',function() {
$(".fp__overlay").css({'z-index':'99999'});
});



